{{#each this}}

<li class="movie-list-item">

  <a href="#" class="movie__link">

    <img src="{{poster}}" class="movie__image">

    <div class="data" data-title="{{title}}" data-voteAverage="{{voteAverage}}" data-voteCount="{{voteCount}}"
      data-overview="{{overview}}" data-popularity="{{popularity}}" data-originalTitle="{{originalTitle}}"
      data-genres="{{dataGenres}}" data-poster="{{poster}}">
      <button hidden type="button" class="btn delete"><svg class="remove-svg" xmlns="http://www.w3.org/2000/svg"
          width="12" height="16" viewBox="0 0 12 16">
          <path fill-rule="evenodd"
            d="M11 2H9c0-.55-.45-1-1-1H5c-.55 0-1 .45-1 1H2c-.55 0-1 .45-1 1v1c0 .55.45 1 1 1v9c0 .55.45 1 1 1h7c.55 0 1-.45 1-1V5c.55 0 1-.45 1-1V3c0-.55-.45-1-1-1zm-1 12H3V5h1v8h1V5h1v8h1V5h1v8h1V5h1v9zm1-10H2V3h9v1z" />
        </svg></button>
    </div>
    <h2 class="movie-description dark">{{title}}</h2>

    {{!-- <div class="data" data-title="{{title}}" data-voteAverage="{{voteAverage}}" data-voteCount="{{voteCount}}"
      data-overview="{{overview}}" data-popularity="{{popularity}}" data-originalTitle="{{originalTitle}}"
      data-genres="{{dataGenres}}" data-poster="{{poster}}">
    </div>
    <h2 class="movie-description dark">{{title}}</h2> --}}


    {{#each genres}}
    <p class="movie-description light">{{this}}</p>
    {{/each}}
    <p class="movie-description light year"> | {{releaseDate}}</p>
    <p class="movie-description vote_averege">{{voteAverage}}</p>

  </a>
</li>

{{/each}}